{
    "name": "zikula/users-module",
    "version": "3.1.0-dev",
    "description": "User account administration",
    "type": "zikula-system-module",
    "license": "LGPL-3.0-or-later",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Zikula",
            "homepage": "https://ziku.la/"
        }
    ],
    "autoload": {
        "psr-4": { "Zikula\\UsersModule\\": "" }
    },
    "require": {
        "php": ">=7.2.5",
        "symfony/config": "^5.2",
        "symfony/console": "^5.2",
        "symfony/contracts": "^5.2",
        "symfony/dependency-injection": "^5.2",
        "symfony/doctrine-bridge": "^5.2",
        "symfony/event-dispatcher": "^5.2",
        "symfony/form": "^5.2",
        "symfony/http-foundation": "^5.2",
        "symfony/http-kernel": "^5.2",
        "symfony/intl": "^5.2",
        "symfony/mailer": "^5.2",
        "symfony/property-access": "^5.2",
        "symfony/routing": "^5.2",
        "symfony/security": "^5.2",
        "symfony/string": "^5.2",
        "symfony/validator": "^5.2",

        "zikula/core-bundle": "self.version",
        "zikula/hook-bundle": "self.version",
        "zikula/formextension-bundle": "self.version",
        "zikula/blocks-module": "self.version",
        "zikula/extensions-module": "self.version",
        "zikula/groups-module": "self.version",
        "zikula/menu-module": "self.version",
        "zikula/search-module": "self.version",
        "zikula/securitycenter-module": "self.version",
        "zikula/settings-module": "self.version",
        "zikula/theme-module": "self.version",
        "zikula/permissions-module": "self.version",
        "zikula/zauth-module": "self.version",
        "zikula/sortable-columns": "1.*"
    },
    "extra": {
        "zikula": {
            "class": "Zikula\\UsersModule\\ZikulaUsersModule",
            "core-compatibility": ">=3.0.0",
            "displayname": "Users Module",
            "url": "users",
            "icon": "fas fa-users-cog",
            "capabilities": {
                "admin": {
                    "route": "zikulausersmodule_useradministration_listusers"
                },
                "user": {
                    "route": "zikulausersmodule_account_menu"
                }
            },
            "securityschema": {
                "ZikulaUsersModule::": "Uname::User ID",
                "ZikulaUsersModule::LiveSearch": "::",
                "ZikulaUsersModule::MailUsers": "::",
                "Accountlinks::": "Block title::",
                "Loginblock::": "Block title::",
                "Onlineblock::": "Block ID::"
            }
        }
    }
}
